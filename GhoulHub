-- Scripts:

local function UTBYTSY_fake_script() -- GrabAK47.AK47 
	local script = Instance.new('LocalScript', GrabAK47)

	GrabAK47.MouseButton1Down:connect(function()
		local args = {
			[1] = workspace.Prison_ITEMS.giver:FindFirstChild("AK-47").ITEMPICKUP
		}
	
		workspace.Remote.ItemHandler:InvokeServer(unpack(args))
	end)
end
coroutine.wrap(UTBYTSY_fake_script)()
local function MWAK_fake_script() -- TpSafePlace.Safe Place 
	local script = Instance.new('LocalScript', TpSafePlace)

	TpSafePlace.MouseButton1Down:connect(function()	
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-19456.1074, -213.206375, 989.955627, 0.645930648, -5.46522045e-08, 0.763396144, 5.9009384e-08, 1, 2.16614087e-08, -0.763396144, 3.10557695e-08, 0.645930648)
	end)
end
coroutine.wrap(MWAK_fake_script)()
local function TCYLCYH_fake_script() -- FastM9.FastM9 
	local script = Instance.new('LocalScript', FastM9)

	FastM9.MouseButton1Down:connect(function()
		local Player = game:GetService('Players').LocalPlayer
		local Backpack = Player.Backpack
	
		local gun = Backpack.M9
	
		gs = require(gun.GunStates)
	
	--[[        Remove this line
	print()
	print('Before')
	for index, v in pairs(gs) do
	    print(index, v)
	end
	]]          -- this one
	
		gs.MaxAmmo = math.huge      -- math.huge is basically Infinite in Lua
		gs.CurrentAmmo = math.huge
		gs.AutoFire = true          -- true = on false = off
		gs.Spread = 0
		gs.FireRate = 0.1 
		gs.Range = math.huge
		gs.Damage = 300
	
	--[[    this one
	print()
	print('After')
	for index, v in pairs(gs) do
	    print(index, v)
	end
	]]      -- and this one if you want it to print out the differences
	end)
end
coroutine.wrap(TCYLCYH_fake_script)()
